# Name: persisted lists
# Description: basic persistent list operations
# Tags: list

data persistedListData {
    Name
    Objects
}

put("persistedListData", "Letters", ['A', 'B'])
put("persistedListData", "Numbers", ['1', '2'])

print(persistedListData)

iterate persistedListData => d {
    print(d)
    print(d.Objects)
    iterate d.Objects => o {
        print(o)
    }
}
